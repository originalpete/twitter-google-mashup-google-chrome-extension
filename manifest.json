{
  "name": "Google + Twitter mashup",
  "version": "1.2.0",
  "description": "View Google and Twitter search results in the Google results screen",
  "permissions": [
    "http://search.twitter.com/*"
  ],
  "background_page" : "background.html",
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "include_globs": [ "http://*.google.*/search\\?*" ],
      "js" : ["jquery.min.js", "script.js"]
    }
  ]
}